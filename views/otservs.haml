%span= will_paginate @otservs, :renderer => BootstrapPagination::Sinatra

.box
  .box-title Otservs
  .box-content.nopadding
    = haml :_search, :layout => false
    %table.datatable.table.table-bordered.table-striped
      %thead
        %tr
          %th Host
          %th Minutes Played
          %th Players
          %th Protocol
      %tbody
        - @otservs.each do |otserv|
          %tr
            %td= otserv_link otserv.id, otserv.name
            %td= otserv.minutes_played
            %td= otserv.num_players
            %td= otserv.protocol